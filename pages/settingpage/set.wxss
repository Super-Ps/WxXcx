.setcontainer {
  background-color: #eee;
}

.body {
  height: 100vh;
  width: 100vw;
  box-sizing: border-box;
  text-align: center;
}

.bodybg {
  height: 15vh;
  width: 100vw;
  background-color: #0e79f3;
}

.bodymain {
  position: relative;
  top: -15vh;
  padding: 0 3vw;
}

.by1 {
  height: 45vh;
  background-color: #fff;
  border-radius: 6px;
  /* box-shadow: 0px 1px 5px 0px #ccc; */
  display: flex;
  flex-direction: column;
  align-items: center;
  padding: 0 5vw;
  box-sizing: border-box;
}

.by2 {
  height: 8vh;
  background-color: #fff;
  border-radius: 6px;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 5vw;
  box-sizing: border-box;
  justify-content: space-between;
  margin-top: 24rpx;
}

.by3 {
  height: 10vh;
  background-color: #fff;
  border-radius: 6px;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 5vw;
  box-sizing: border-box;
  justify-content: center;
  margin-top: 24rpx;
}

.by4 {
  height: 8vh;
  background-color: #fff;
  border-radius: 6px;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0 5vw;
  box-sizing: border-box;
  justify-content: flex-start;
  margin-top: 24rpx;
}

.by5 {
  background-color: #fff;
  position: fixed;
  bottom: 0;
  width: 100vw;
  height: 15vw;
  left: 0;
  display: flex;
  justify-content: center;
  align-items: center;
}

.main_item:not(:last-child) {
  border-bottom: solid 1px #ededed;
}

.main_item {
  width: 100%;
  align-items: center;
  display: flex;
  flex-flow: row nowrap;
}

.row1 {
  height: 15vh;
  justify-content: space-between;
}

.row2 {
  height: 10vh;
  justify-content: space-between;
}

.row3 {
  height: 10vh;
  justify-content: space-between;
}

.row4 {
  height: 10vh;
  justify-content: space-between;
}

.it1 {
  font-size: 28rpx;
  color: #555;
}

.it2 {
  font-size: 28rpx;
  color: #000;
  font-weight: bold;
  margin-left: 30rpx;
}

.it3 {
  font-size: 28rpx;
  color: #0e79f3 !important;
}

.it4 {
  font-size: 28rpx;
  color: #555;
  margin-left: 5rpx;
}

.it5 {
  font-size: 32rpx;
  font-weight: bold;
}

.button {
  width: 94vw;
  padding: 11rpx 0;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  border-radius: 5vw;
  background: #0e79f3;
  font-size: 32rpx;
}

.item {
  display: flex;
  flex-direction: column;
}

.pointTitle {
  font-size: 24rpx;
  color: #555;
}

.point {
  font-size: 36rpx;
  font-weight: bold;
}

.addBtn {
  font-size: 32rpx;
  color: #0e79f3;
  font-weight: bold;
}
.showCity{
  left: 0!important;
}
.cityContainer {
  background-color: #ffffff;
  width: 100vw;
  height: 100vh;
  position: fixed;
  top: 0;
  left: 100vw;
  z-index: 9;
  transition: all .5s;
}